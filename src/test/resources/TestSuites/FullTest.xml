<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite parallel="true" thread-count="4" name="Full Test Suite">
	
	<test name="LoginTest">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="tests.LoginPageTest" />
		</classes>
	</test> 
	
	<test name="NegativeLoginTest">
		<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="tests.NegativeLoginTest" />
		</classes>
	</test> 
	
	<test name="FrameTest">
		<parameter name="browser" value="msedge"></parameter>
		<classes>
			<class name="tests.FrameTest" />
		</classes>
	</test>
	
	
	<test name="DropDownTest">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="tests.DropDownTest" />
		</classes>
	</test>
	
	
	<test name="CheckBoxTest">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="tests.CheckboxTest" />
		</classes>
	</test>
	
</suite> 
